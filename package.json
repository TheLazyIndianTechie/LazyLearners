    "@tiptap/react": "^3.6.2",
    "@tiptap/starter-kit": "^3.6.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/fluent-ffmpeg": "^2.1.27",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^3.0.2",
